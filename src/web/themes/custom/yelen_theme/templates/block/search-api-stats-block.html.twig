<div class="most-popular-terms__list">
  {% for keyword, num in stats %}
  <a class="term card" href="{{ path }}?{{ param_name }}={{ keyword }}">
    <div class="card-body">
      <span >
        {{ keyword }} {#  ({{ num }}) #}
      </span>
    </div>
  </a>
  {% endfor %}
</div>
